{
	"name": "placeholder",
	"version": "0.0.0",
	"description": "placeholder",
	"homepage": "https://github.com/username/placeholder",
	"main": "build/placeholder.js",
	"module": "src/index.js",
	"sideEffects": false,
	"license": "Zlib",

	"keywords": [],

	"author": {
		"name": "username",
		"email": "username@domain"
	},

	"repository": {
		"type": "git",
		"url": "https://github.com/username/placeholder.git"
	},

	"bugs": {
		"url": "https://github.com/username/placeholder/issues"
	},

	"files": [
		"src",
		"build"
	],

	"scripts": {
		"lint": "npm run lint:lib && npm run lint:test",
		"lint:config": "eslint *.js",
		"lint:lib": "eslint src",
		"lint:test": "eslint test",
		"build": "rollup -c",
		"build:production": "cross-env NODE_ENV=production rollup -c",
		"doc": "esdoc",
		"test": "ava",
		"prepack": "npm run lint && npm run build:production"
	},

	"engines": {
		"node": ">=4.0.0",
		"npm": ">=5.0.0"
	},

	"ava": {
		"failFast": true,
		"files": ["test/**/*.js"]
	},

	"eslintConfig": {
		"extends": "delta"
	},

	"devDependencies": {
		"@babel/core": "7.x.x",
		"@babel/preset-env": "7.x.x",
		"ava": "0.x.x",
		"cross-env": "5.x.x",
		"esdoc": "1.x.x",
		"esdoc-standard-plugin": "1.x.x",
		"eslint": "5.x.x",
		"eslint-config-delta": "0.x.x",
		"rollup": "0.x.x",
		"rollup-plugin-babel": "4.x.x",
		"rollup-plugin-babel-minify": "6.x.x",
		"rollup-plugin-node-resolve": "3.x.x"
	}
}
